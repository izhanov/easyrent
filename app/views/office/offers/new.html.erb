<div class="d-flex justify-content-between align-items-center">
  <h1><%= t(".title") %></h1>
  <%= link_to office_car_park_car_path(@car_park, @car), class: "base-button base-button--info base-button--sm" do %>
    <%= t(".back") %>
  <% end %>
</div>


<div class="container">
  <%= turbo_frame_tag @offer do %>
