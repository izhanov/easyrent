    <span class="text-secondary small fs-5 font-monospace"><%= localize_consumable_title(@consumable.title) %></span>
    <span class="text-secondary small fs-5 font-monospace"><%= @car.mark_title %></span>
    <span class="text-secondary small fs-5 font-monospace"><%= @car.plate_number %></span>
  </h1>
  <%= link_to office_car_park_car_consumable_path(@car_park, @car, @consumable), class: "btn btn-sm btn-outline-primary" do %>
